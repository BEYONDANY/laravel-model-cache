{
    "name": "xuhaohao/laravel-model-cache",
    "description": "auto caching mode db",
    "autoload": {
        "psr-4": {
            "Xuhaohao\\LaravelModelCache\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "xuhh2",
            "email": "xuhh2@lenovo.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "genealabs/laravel-pivot-events": "^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.10",
        "fakerphp/faker": "^1.11",
        "laravel/nova": "^3.9",
        "orchestra/testbench-browser-kit": "^6.0",
        "orchestra/testbench": "^6.0",
        "php-coveralls/php-coveralls" : "^2.2",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^9.0",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/thanks": "^1.2",
        "laravel/legacy-factories": "^1.0"
    },

    "autoload-dev": {
        "classmap": [
            "tests/database/factories",
            "tests/database/seeds"
        ],
        "psr-4": {
            "Xuhaohao\\LaravelModelCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xuhaohao\\LaravelModelCache\\Providers\\Service"
            ]
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "symfony/thanks": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
